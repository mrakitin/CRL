        "type": "tuple"
    },
    "teta0": {
        "default": 6e-05,
        "help": "divergence of the beam before CRL [rad]",
        "type": "float"
    },
    "use_numpy": {
        "default": false,
        "help": "use NumPy for operations with matrices",
        "type": "bool"
    }
